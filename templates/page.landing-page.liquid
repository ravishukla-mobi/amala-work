{% layout none %}
<head>
   <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, height=device-height, minimum-scale=1.0, maximum-scale=5.0">
    <meta name="theme-color" content="{{ settings.accent_color }}">
<meta name="google-site-verification" content="p-za_fqa5pcRWFrX0uXGQWK5g4wE3QJeMWqlUMYSajU" />
{%- if settings.favicon -%}
      <link rel="shortcut icon" href="{{ settings.favicon | img_url: '96x96' }}" type="image/png">
    {%- endif -%}
  <title>Amala Earth | Curated Market Place for Conscious Living</title>
  {%- if page_description -%}
  <meta name="description" content="{{ page_description | escape }}">
  {%- endif -%}
  {% render 'social-meta-tags' %}
    {% render 'css-variables' %}
    <link rel="preload" as="script" href="{{ 'theme.js' | asset_url }}">
   <link rel="stylesheet" href="{{ 'theme.css' | asset_url }}">
  {% if template contains 'blog' or template contains 'article' %}
  {{ 'blogpage.css' | asset_url | stylesheet_tag }}
  {% endif %}
  <script src="{{ 'theme.js' | asset_url }}" defer></script>
    <script src="{{ 'custom.js' | asset_url }}" defer></script>
</head>
<style>
@import url('https://fonts.googleapis.com/css2?family=Libre+Baskerville:ital,wght@0,400;0,700;1,400&display=swap');
*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    outline: none;
}
html{
    height: 100%;
}
h1, h2, h3, h4, h5, h6{
    font-family: 'Libre Baskerville', serif;
}
p{
    font-size: 1em;
    line-height: 32px;
    letter-spacing: 1px;
}
body{
    font-family: 'Libre Baskerville', serif;
    background-image: url(https://cdn.shopify.com/s/files/1/0523/3252/7770/files/left-bg.png?v=1620812984), url(https://cdn.shopify.com/s/files/1/0523/3252/7770/files/right-bg.png?v=1620812984);
    background-repeat: no-repeat;
    background-position: bottom left, bottom right;
    background-size:9%;
}
  @media only screen and (min-width: 1580px) {
    body{
      height:100%;
    }
  }
.container{
    max-width: 1000px;
    width: 100%;
    margin: auto;
}
header{
    padding: 30px 0;
    text-align: center;
    display: grid;
}
header h1{
    color: #b7b7b7;
    font-weight: 400;
    font-size: 2rem;
    letter-spacing: 1px;
}
.banner-section {
    padding: 30px 0 0;
    background-image: url(https://cdn.shopify.com/s/files/1/0523/3252/7770/files/banner-bg.png?v=1620812985);
    background-color: #ae9173;
    text-align: center;
    background-repeat: repeat-x;
    background-position: bottom;
    height: 330px;
    background-size: 30%;
}
.banner-section .container{
    margin: 0 auto;
}
.logo svg{
    max-width: 400px;
    height: auto;
}
.banner-section h2{
    display: block;
    font-size: 2.5rem;
    font-weight: 400;
}
.title--block {
    text-align: center;
    display: block;
    padding: 2em 0 0;
    color: #a87f73;
    font-size: 1.3em;
    letter-spacing: 1px;
    font-weight: 400;
}
.title--block h1 {
    display: block;
    margin: 30px 0;
    padding: 0 20%;
    letter-spacing: 2px;
    font-weight: 300;
    font-size: 1.6em;
    color: #404040;
    line-height: 42px;
}
.about--block {
    display: grid;
}
.about--block h2{
    display: block;
    font-size: 2em;
    text-align: center;
    font-weight: 500;
    color: #353c42;
    letter-spacing: 1px;
    line-height: 30px;
    margin-bottom: 50px;
}
.about--block h3 {
    color: #bf9781;
    text-align: center;
    margin: 20px 0 15px;
    font-size: 2em;
    font-weight: 400;
    letter-spacing: 1px;
}
.about--block p {
    padding: 0 22%;
    text-align: justify;
    font-size: 0.8em;
    line-height: 24px;
}
.mission--row {
    margin: 60px 0 20px;
    text-align: center;
    padding: 0 20%;
}
.mission--row strong {
    display: block;
    color: #bf9781;
    margin-bottom: 10px;
    font-weight: 300;
    letter-spacing: 1px;
    font-size: 1.2em;
}
.notify-section .container {
    display: grid;
}
.notify-section_form {
    margin: auto;
    width: 50%;
}
.notify-section_form input[type="email"] {
    height: 50px;
    padding-left: 15px;
    border: 1px solid #ae9173;
    width: 80%;
    padding: 10px 20px;
    font-size: 0.8em;
    font-family: 'Libre Baskerville', serif;
    margin-bottom: 20px;
    -webkit-appearance: none;
    color: #000 !important;
    text-align: center;
    letter-spacing: 1px;
    border-radius:0;
}
.notify-section_form input[type="email"]::-webkit-input-placeholder { /* Edge */
    color: #ae9173;
  }
  .notify-section_form input[type="email"]:-ms-input-placeholder {
    color: #ae9173;
  }
  .notify-section_form input[type="email"]::placeholder {
    color: #ae9173;
  }
.notify-section_form input[type="submit"]{
    border-radius: 10px;
    background-color: #ae9173;
    color: #fff;
    border: 0;
    margin-bottom: 0;
    padding: 16px 32px;
    font-size: 14px;
    font-weight: 500;
    line-height: 14px;
    box-shadow: none;
    transition: all 0.25s ease-in-out;
    font-family: 'Libre Baskerville', serif;
    letter-spacing: 1px;
    -webkit-appearance: none;
    cursor: pointer;
}
.notify-section_form input[type="submit"]:hover{
    background-color: #ab8671;
}
.notify-block {
    display: grid;
    text-align: center;
    margin:0px 0 40px;
}
.notify-section_form {
    margin: 10px auto 0;
    width: 50%;
}
.notify-block h3{
    display: block;
    margin-bottom: 10px;
    font-weight: 400;
    color: #bf9781;
    font-size: 1.5em;
}
.notify-block p {
    font-size: 0.8em;
    line-height: 24px;
    color: #7f7f7f;
    letter-spacing: .5px;
}
footer {
    text-align: center;
    margin-top: 0;
    color: #bf9781;
    position: absolute;
    bottom: 10px;
    width: 100%;
}
  footer .container p{
    display: inline-block;
  }
  footer .container p a{
    padding:0;
    margin:0;
  }
  footer .container p a:before{
    display:none;
  }
  footer .container a{
    display: inline-block;
    color: #bf9781;
    text-decoration: none;
    margin-left: 10px;
    padding-left: 20px;
    position: relative;
    font-size: 1em;
    transition: color 0.2s ease-in-out;
  }
  footer .container a:hover{
  	color:#a87f73;
  }
  footer .container a:before{
    content: "";
    position: absolute;
    width: 6px;
    height: 6px;
    border-radius: 100%;
    background: #bf9781;
    left: 0;
    top: 50%;
    transform: translateY(-50%);
  }
  
  
@media (max-height: 50rem) {
    footer{
        position: relative;
    }
}
.mce_inline_error {
    margin-bottom: 10px;
    font-size: 12px;
    color: #f00;
}
div#mce-success-response {
    margin-bottom: 11px;
    color: green;
    font-size: 11px;
}
#mce-error-response{
    font-size: 12px;
    line-height: 16px;
    margin-bottom: 10px;
    color: #7f7f7f;
}
#mce-error-response a{
    color: #a87f73;
}
@media only screen and (max-width: 1580px) {
    .title--block h1{
        padding: 0 30%;
        font-size: 1.3em;
        line-height: 38px;
    }
    .title--block{
        padding: 1.5em 0 0;
    }
    .banner-section{
        height: 310px;
    }
}
@media only screen and (max-width: 1440px) {
    body{
        background-size: 14%;
    }
    .banner-section{
        height: 300px;
    }
}
@media only screen and (max-width: 1024px){
    .banner-section {
        height: 250px;
    }
    .title--block {
        padding: 2em 0 0;
    }
    .title--block h1{
        margin: 15px 0;
    }
    .banner-section{
        padding: 15px 0 0;
    }
}
@media screen and (min-width: 768px) and (max-width: 991px){
    .container{
        margin: 0;
    }
}
@media screen and (min-width: 768px) and (max-width: 800px){
    .text-container {
        display: grid;
    }
    .title--block {
        padding: 1em 0 0;
    }
}
@media screen and (min-width: 568px) and (max-width:767px){
    footer{
        position: relative;
    }
}
@media only screen and (max-width: 991px) {
    html, body{
        height: auto;
    }
    body{
        background-size: 20%;
    }
    .title--block h1{
        padding: 0 15%;
    }
}
@media only screen and (max-width: 767px) {
    header{
        padding: 20px 0;
    }
    header h1, .banner-section h2{
        font-size: 1.5rem;
    }
    .banner-section {
        padding-top: 20px;
        background-size: 70%;
        height: 280px;
    }
    .logo svg{
        max-width: 280px;
    }
    .about--block h2{
        font-size: 1.6em;;
    }
    .about--block h3{
        font-size: 1.5em;
    }
    .title--block{
        padding: 1em 0 0;
    }
    .notify-section_form input[type="text"]{
        text-align: center;
    }
    main{
        background-size: 21%;
    }
    .text-container{
        height: auto;
    }
}
@media only screen and (max-width: 568px) {
  html, body {
    height: 100%;
  }
    header{
        padding: 20px 0;
    }

    header h1{
        font-size: 1.4rem;
        letter-spacing: 1px;
    }
    section{
        padding: 0 10%;
    }
    main{
        background-size: 30%;
    }
    
    .logo svg{
        max-width: 100%;
        padding: 0 20%;
    }
    body {
        background-size: 32%;
    }
    .banner-section h2{
        font-size: 1.5rem;
    }
    .banner-section {
        height: 200px;
        padding: 20px 0 0;
    }
    .title--block{
        font-size: 1em;
        letter-spacing: .5px;
        padding-top: 2em;
    }
    .notify-section_form{
        width: 100%;
    }
    .notify-section_form input[type="submit"]{
        width: auto;
        font-size: .8em;
        padding: 15px 30px;
        height: auto;
    }
    .about--block h2{
        font-size: 1.2em;
    }
    .about--block h3, .mission--row strong{
        font-size: 1em;
    }
    .mission--slogn strong{
        line-height: 24px;
    }
    footer .container a {
        font-size: .8em;
        line-height: 24px;
        letter-spacing: .5px;
    }
  footer .container a{
    margin-left: 5px;
    padding-left: 15px;
  }
    .about--block p{
        padding: 0 5%;
    }
    .title--block h1{
        line-height: 32px;
        font-size: 1.5em;
        padding:0;
        letter-spacing: 0.5px;
        margin: 30px 0;
    }
    .notify-section_form input[type="email"]{
        width: 90%;
    }
  footer{
    position: absolute;
    bottom: 10px;
  }
}
@media only screen and (max-width: 340px) {
    .banner-section{
        height: 150px;
    }
    .title--block h1{
        line-height: 28px;
        font-size: 1.2em;
    }
}
</style>
