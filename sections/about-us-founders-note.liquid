            font-size: 14px;
            line-height: 21px;
        }
        .content_para_1 {
            margin-top: 21px;
        }
        .founder_primary_title, .founder_secondary_title {
            margin-left: 0;
        }
        .content_para_1 {
            margin-top: 20px;
        }
    }
    @media screen and (max-width: 420px) {
        .content_para_1, .content_para_2 {
