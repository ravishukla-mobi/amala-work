    @media screen and (max-width: 450px) and (min-width: 420px) {
        .founder_note_cus {
            background-size: cover;
        }
    }
    @media screen and (max-width: 390px) and (min-width: 370px) {
        .founder_note_cus {
            background-size: cover;
        }
    }

    @media screen and (min-width: 340px) and (max-width: 370px) {
        .content_para_1, .content_para_2 {
