
<div class="founder_note_cus">
    <div class="founder_content_parent">
        <div class="founder_title">
            <h2 class="founder_primary_title">{{section.settings.founder_primary_title}}</h2>
            <h1 class="founder_secondary_title">{{section.settings.founder_seconday_title}}</h1>
        </div>
        <div class="founder_content">
            <div class="founder_content_par">
                <div class="content_para_1">{{section.settings.description_para_1}}</div>
                <div class="quoted_text">
                    <div class="quoted_text">
                        <span class="title-left_quotes">&ldquo;</span><span>{{section.settings.description_couted_text}}</span><span class="title-right_quotes">&rdquo;</span> 
                    </div>
                </div>
                <div class="content_para_2">{{section.settings.description_para_2}}</div>
                <h2 class="founder_signature founders_signature_desk">
                    {{section.settings.founder_signature}}
                </h2>
                <h2 class="founders_signature_mob">
