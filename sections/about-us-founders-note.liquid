    line-height: 20px;
}
.content_para_1 {
    margin-top: 5px;
}
.quoted_text {
    font-size: 26px;
    font-weight: 500;
    line-height: 25px;
    letter-spacing: 0.04em;}
    .quoted_text svg {
    height: 30px;
    width: 30px;
}
    }
    @media screen and (max-width: 390px) {
        .founders_signature_mob {
            display: block;
