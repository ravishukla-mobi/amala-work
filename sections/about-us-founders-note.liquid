{% schema %}
{
    "name": "Founders Note",
    "settings": [
        {
            "type": "image_picker",
            "id": "background_desk_img",
            "label": "Desktop Background Image"
        },
        {
            "type": "color",
            "id": "background_body_color",
            "label": "Body background color",
            "default": "#F2E5D8"
        },
        {
            "type": "image_picker",
            "id": "background_mob_img",
            "label": "Mobile Background Image"
        },
        {
            "type": "text",
            "id": "founder_primary_title",
            "label": "Primary Founder Heading",
            "default": "Note from"
        },
        {
            "type": "text",
            "id": "founder_seconday_title",
            "label": "Primary Founder Heading",
            "default": "our founder"
        },
        {
            "type": "richtext",
            "id": "description_para_1",
            "label": "Note Content above quoted text"
        },
        {
            "type": "text",
            "id": "description_couted_text",
            "label": "Quoted text",
            "default": "Transformation is a word that’s close to my heart"
        },
        {
            "type": "richtext",
            "id": "description_para_2",
            "label": "Note Content below quoted text"
        },
        {
            "type": "richtext",
            "id": "founder_signature",
            "label": "Founder's signature"
        },
        {
            "type": "richtext",
            "id": "founder_signature_mob",
            "label": "Founder's signature Mobile view"
        }
    ],
    "presets": [
        {
            "name": "Founder Note"
        }
    ] 
}
{% endschema %}