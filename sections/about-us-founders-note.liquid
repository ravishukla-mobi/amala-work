            width: 100%;
        }
        .founder_title{
            margin-top: 10%;
        }
        .founder_content_par {
            text-align: center;
        }
        .content_para_1, .content_para_2 {
            font-size: 15px;
            line-height: 24px;
        }
        .quoted_text {
            font-size: 29px;
