<div class="home_featured-in_section section" data-section-id="{{ section.id }}" data-section-type="home-featured-in-list">
    <div class="container">
        {%- if section.settings.title != blank or section.settings.subtitle != blank -%}
        <header class="landing_section_header">
        {% if section.settings.subtitle != blank %}
        <h5 class="landing_subheading">{{section.settings.subtitle}}</h5>
        {% endif %}
        {% if section.settings.title != blank %}
        <h4 class="landing_heading">{{section.settings.title}}</h4>
        {% endif %}
        </header>
        {%- endif -%}
        <div class="home_featured_list_silder">
            {%- for block in section.blocks -%}
            <div id="block_{{block.id}}" class="featured_list_block">
                <a href="{{ block.settings.link }}" target="_blank">
                {%- if block.settings.image != blank -%}
                {%- capture supported_sizes -%}{%- render 'image-size', sizes: '100,200,300,400,500', image: block.settings.image -%}{%- endcapture -%}
                {%- assign image_url = block.settings.image | img_url: '1x1' | replace: '_1x1.', '_{width}x.' -%}
                <div class="brand_image">
                  <div class="brand-image_wrapper">
                      <img class="amala-brand_image lazyload image--fade-in" alt="{{ block.settings.image.alt | escape }}" data-src="{{ image_url }}" data-widths="[{{ supported_sizes }}]" data-sizes="auto">
                      <noscript>
                      <img src="{{ block.settings.image | img_url: '300x' }}" alt="{{ block.settings.image.alt | escape }}">
                      </noscript>
                  </div>
                </div>
                {%- endif -%}
                {% if block.settings.title != blank %}
                <div class="brand_name">
                    <h2>{{block.settings.title}}</h2>                
                    <svg width="12" height="13" viewBox="0 0 12 13" fill="none" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
                    <rect y="0.224609" width="12" height="12" fill="url(#pattern0)" fill-opacity="0.4"/>
                    <defs>
                    <pattern id="pattern0" patternContentUnits="objectBoundingBox" width="1" height="1">
                    <use xlink:href="#image0_1781_10325" transform="scale(0.00195312)"/>
                    </pattern>
                    <image id="image0_1781_10325" width="512" height="512" xlink:href="data:image/png;base64,"/>
                    </defs>
                    </svg>
                </div>
                {% endif %}
                </a>
            </div>
            {%- endfor -%}
        </div>
    </div>
</div>

{% style %}
.home_featured_list_silder{
    position: relative;
    display: flex;
    overflow: hidden;
    width: 100%;
    z-index: 1;
}
.home_featured_list_silder:not(.flickity-enabled){
    overflow-y:auto;
}
.home_featured_list_silder:after {
  content: 'flickity';
  display: none; /* hide :after */
}
.featured_list_block{
  flex: none;
  width:calc(23% - 20px);
  margin:7px 20px 7px 0;
  border-radius: 10px;
  overflow: hidden;
  box-shadow:0px 4px 8px 0px rgba(0, 0, 0, 0.10);
}
.featured_list_block a {
  display: block;
}
.featured_list_block .brand_image{
  display: block;
  width: 100%;
  padding: 30px;
  border: 2px solid #F2E5D8;
  border-radius: 10px 10px 0 0;
}
.brand-image_wrapper{
  position:relative;
  overflow:hidden;
  width: 100%;
  padding-top:100%;
  display:block;
}
.brand-image_wrapper:before {
  content: '';
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: transparent;
  z-index: 1;
}
.brand-image_wrapper img{
  position: absolute;
  width:100%;
  height:auto;
  z-index:2;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.featured_list_block .brand_name {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  background-color: #F2E5D8;
  padding: 10px 15px;
}
.featured_list_block .brand_name h2 {
  font-size: 18px;
  width: calc(100% - 18px);
  margin-bottom: 0;
  text-align: center;
  line-height: 1.2;
  transition:all 0.3s;
}
.featured_list_block:hover .brand_name h2{
  color: #946c56;
}
.featured_list_block .brand_name svg {
    width: 18px;
    height: 18px;
    margin-bottom: -2px;
}
@media screen and (max-width:1199px){
  .featured_list_block{
    width:calc(30% - 20px);
  }
  .featured_list_block .brand_name h2{
    font-size: 16px;
    width: calc(100% - 16px);
  }
  .featured_list_block .brand_name svg {
    width: 16px;
    height: 16px;
  }
}
@media screen and (max-width:991px){
  .featured_list_block .brand_image {
    padding: 20px 30px;
  }
  .featured_list_block .brand_name h2{
    font-size: 14px;
    width: calc(100% - 12px);
    padding-right:5px;
  }
  .featured_list_block .brand_name svg {
    width: 12px;
    height: 12px;
    margin-bottom: 0;
  }
}
@media screen and (max-width:767px){
.home_featured-in_section .container {
    padding-right: 0;
}
.featured_list_block{
  width:calc(45% - 15px);
  margin-right:15px;
}
}
@media screen and (max-width:640px){
  .featured_list_block .brand_name{
    padding: 10px 7px;
  }
  .featured_list_block .brand_name h2 {
    font-size: 13px;
  }
}
@media screen and (max-width:369px){
  .featured_list_block .brand_name h2 {
    font-size: 12px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
  }
}
{% endstyle %}
{% schema %}
    {
      "name": "Home featured in",
      "settings": [
        {
          "type":"text",
          "id":"subtitle",
          "label":"Subheading"
        },
        {
          "type":"text",
          "id":"title",
          "label":"Heading",
          "default":"Featured in"
        }
      ],
      "blocks":[
        {
          "type": "brand_logo",
          "name": "Brand logo",
          "settings": [
            {
              "type":"image_picker",
              "id":"image",
              "label":"Image"
            },
            {
              "type":"text",
              "id":"title",
              "label":"Heading"
            },
            {
              "type":"url",
              "id":"link",
              "label":"link"
            }
          ]
        }
      ],
      "presets": [
        {
          "name": "Home featured in",
          "category": "Custom"
        }
      ]
    }
  {% endschema %}