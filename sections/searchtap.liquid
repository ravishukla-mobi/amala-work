<script>
    document.addEventListener('DOMContentLoaded', function() {
        $(document).on('click', '.product-inner .product-top a, .product-inner .product-title a', function(e) {
            var $product = $(this).parents('.product-inner');
            var $form = $product.find('form');
            var price = $product.find('.product-prices.no-sale').text();
            price = Number(price.replace(/[^0-9.-]+/g,""));
            dataLayer.push({
                event: "productClick",
                ecommerce: {
                    click: {
                        actionField: {
                            list: window.location.href, 
                            action: "click"
                        },
                        products: [
                            {
                                id: $product.find('.wk-add-product').attr('data-wk-add-product'),
                                name: $product.find('.product-title').text(),
                                price: price,
                                brand: $product.find('.st-vendor > a').text(),
                                category: $('.collection__title').text(),
                                variant: $form.find('input[name="id"]').val(),
                                position: $product.parent().index() + 1
                            }
                        ]
                    }
                },
            })
        });
    });
  </script>

