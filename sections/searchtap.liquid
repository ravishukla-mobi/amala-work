    var _assuredInterval = setInterval(function() {
      {%- if settings.ProductDeliveryAssured_enable -%}
        let skus = [],cSkuvalues = [],SkuCname = 'ProductDeliveryAssured',exSkuminute = parseInt(`{{settings.deliveryassured_minute}}`),getSkuCookies = getCookie(SkuCname),retrievedObj = '',retrievedObjSku = [];
        if(getSkuCookies != null && getSkuCookies != '' && getSkuCookies != undefined) {
            retrievedObj = JSON.parse(getSkuCookies);
            $.each(retrievedObj, function(key, item) {
                cSkuvalues.push(item);
                retrievedObjSku.push(item.sku);
                let $item = $('#products-listing .product-inner [value="' + item.sku + '"]');
                let $inner = $item.parents('.product-inner');
                $inner.attr('data-assured-processed', true);
                if (item.assured) {
                    if ($inner.find('.product-images .product-meta__assured_delivery').length == 0) {
                    $inner.find('.product-images').append('<span class="product-meta__assured_delivery"><img src="{{ 'plp-assured-delivery.svg' | asset_url }}"></span>');
                    }
                }
            });
        }
        $('#products-listing .product-inner').each(function() {
            if (!$(this).attr('data-assured-processed')) {
                let variantId = $(this).find('[name="id"]').val();
                if (variantId) {
                    if(retrievedObj != ''){
                        if(retrievedObjSku.includes(variantId)){
                        }else{
                            skus.push(variantId);
                        }
                    }else{
                        skus.push(variantId);
                    }
                }
            }
        });
  
        if (skus.length) {
            getAssuredResponse(skus, function(response) {
            var data = $.parseJSON(response);
            if (data && data.data && data.data.length > 0) {
                var products = data.data
                $.each(products, function(key, product) {
                var $item = $('#products-listing .product-inner [value="' + product.sku + '"]');
                var $inner = $item.parents('.product-inner');
                $inner.attr('data-assured-processed', true);
                if (skus.includes(product.sku) && product.assured) {
                    if ($inner.find('.product-images .product-meta__assured_delivery').length == 0) {
                    $inner.find('.product-images').append('<span class="product-meta__assured_delivery"><img src="{{ 'plp-assured-delivery.svg' | asset_url }}"></span>');
                    }
                }
                cSkuvalues.push(product);
                });
                let uniqs = {},
                arrFiltereds = cSkuvalues.filter(obj => !uniqs[obj.sku] && (uniqs[obj.sku] = true)),
                setcSkuvalues = JSON.stringify(arrFiltereds);
                setCookie(SkuCname, setcSkuvalues, exSkuminute);
            }
            });
        }
        {%- endif -%}

        let vendors = [];
        $('#products-listing .product-inner').each(function() {
            if (!$(this).attr('data-assured-processed')) {
                let vendorName = $(this).find('.st-vendor a').text(); 
                if (vendorName){
                    vendors.push(vendorName);
                }
            }
        });
        if (vendors.length) {
            $.each(vendors, function(key, cvendor) {
                $.each(theme.allvendors, function(key, vendor) {
                    if(vendor.vendor_name == cvendor && $inner.find('.product-bottom .freebies_vendor_msg')){
                        $inner.find('.product-bottom').append(`<span class="freebies_vendor_msg"><span>Freebie on all orders above <span class="freebies_price"><span class="rupee-symbol">₹</span> ${vendor.value}</span>.</span></span>`);
                    }
                })
            });
        }
    }, 2000);
  });
</script>
